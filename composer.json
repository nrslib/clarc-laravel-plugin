{
    "name": "nrslib/clarc-laravel-plugin",
    "repositories": [
        {
            "type": "path",
            "url": "../clarc/src",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^7.2",
        "nrslib/cfg": "dev-master",
        "nrslib/clarc-php-core": "dev-master",
        "illuminate/support": "^6.2",
        "laravel/framework": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "nrslib\\ClarcLaravelPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "nrslib\\ClarcLaravelPluginTests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "nrslib\\ClarcLaravelPlugin\\ClarcProvider"
            ]
        }
    }
}
